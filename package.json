{
  "name": "Frame",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node_modules/react-native/packager/packager.sh",
    "test-watch": "nodemon -x babel-node js/test/ | tap-colorize -",
    "test-debug": "forever -c \"babel-node --debug-brk\" js/test",
    "test": "babel-node js/test"
  },
  "dependencies": {
    "es6-promisify": "^1.1.1",
    "js-base64": "^2.1.8",
    "moment": "^2.10.3",
    "node-uuid": "^1.4.3",
    "nuclear-js": "git://github.com/optimizely/nuclear-js.git",
    "query-string": "^2.2.0",
    "react-native": "0.6.0",
    "react-native-button": "^1.2.0",
    "react-native-camera": "jtremback/react-native-camera",
    "react-native-fs": "latest",
    "regenerator": "^0.8.30"
  },
  "devDependencies": {
    "babel-eslint": "^3.1.5",
    "eslint": "^0.21.2",
    "eslint-plugin-react": "^2.3.0",
    "forever": "^0.14.1",
    "mock": "^0.1.1",
    "mock-require": "^1.0.5",
    "rn-ip-replace": "^1.4.0",
    "tap-colorize": "^1.2.0",
    "tape": "^4.0.0"
  },
  "eslintConfig": {
    "plugins": [
      "react"
    ],
    "parser": "babel-eslint",
    "env": {
      "browser": true,
      "node": true
    },
    "rules": {
      "react/display-name": 1,
      "react/jsx-boolean-value": 1,
      "react/jsx-quotes": 1,
      "react/jsx-no-undef": 2,
      "react/jsx-sort-props": 0,
      "react/jsx-sort-prop-types": 0,
      "react/jsx-uses-react": 1,
      "react/jsx-uses-vars": 1,
      "react/no-did-mount-set-state": 1,
      "react/no-did-update-set-state": 1,
      "react/no-multi-comp": 0,
      "react/no-unknown-property": 1,
      "react/prop-types": 1,
      "react/react-in-jsx-scope": 1,
      "react/self-closing-comp": 1,
      "react/sort-comp": 1,
      "react/wrap-multilines": 1,
      "no-shadow": 0,
      "dot-notation": 0,
      "no-use-before-define": 0,
      "eqeqeq": 2,
      "semi": 0,
      "strict": 0,
      "curly": 2,
      "camelcase": 0,
      "no-unused-vars": 1,
      "no-unused-expressions": 0,
      "quotes": [
        2,
        "single"
      ],
      "ecmaFeatures": {
        "jsx": true
      },
      "no-underscore-dangle": 0,
      "comma-dangle": 0
    }
  }
}

{
  "name": "SiteCamera",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node_modules/react-native/packager/packager.sh",
    "babel": "babel js -w --stage=1 --out-dir built",
    "ipreplace": "rn-ip-replace"
  },
  "eslintConfig": {
    "plugins": [
      "react"
    ],
    "parser": "babel-eslint",
    "env": {
      "browser": true,
      "node": true
    },
    "rules": {
      "react/display-name": 1,
      "react/jsx-boolean-value": 1,
      "react/jsx-quotes": 1,
      "react/jsx-no-undef": 2,
      "react/jsx-sort-props": 0,
      "react/jsx-sort-prop-types": 0,
      "react/jsx-uses-react": 1,
      "react/jsx-uses-vars": 1,
      "react/no-did-mount-set-state": 1,
      "react/no-did-update-set-state": 1,
      "react/no-multi-comp": 0,
      "react/no-unknown-property": 1,
      "react/prop-types": 1,
      "react/react-in-jsx-scope": 1,
      "react/self-closing-comp": 1,
      "react/sort-comp": 1,
      "react/wrap-multilines": 1,
      "no-shadow": 0,
      "no-use-before-define": 0,
      "eqeqeq": 2,
      "semi": 0,
      "strict": 0,
      "curly": 2,
      "camelcase": 0,
      "no-unused-vars": 1,
      "quotes": [
        2,
        "single"
      ],
      "ecmaFeatures": {
        "jsx": true
      },
      "no-underscore-dangle": 0,
      "comma-dangle": 0
    }
  },
  "dependencies": {
    "react-native": "^0.4.4",
    "react-native-camera": "^0.2.5"
  },
  "rnIpReplace": {
    "files": [
      "./iOS/AppDelegate.m"
    ]
  },
  "devDependencies": {
    "babel": "^5.4.7",
    "babel-eslint": "^3.1.5",
    "eslint": "^0.21.2",
    "eslint-plugin-react": "^2.3.0",
    "rn-ip-replace": "^1.4.0"
  }
}
